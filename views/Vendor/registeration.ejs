function check_agent_code(){
    let agent_id = $('#agentid').val()
    
    $.post('/api/check-bde-code',{agent_id},data=>{
        // alert(data.msg)
        if(data.msg =='invalid'){
            let table = `<span style='color:red'>Invalid Executive Code</span>`
            $('#result').html(table)

        }
        else{
            let table = `<span style='color:green'>Success</span>`
            $('#result').html(table)

        }
    })
}





