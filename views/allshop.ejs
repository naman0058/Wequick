<%- include ('./otherheader.ejs') %>



        <!-- Start of Main -->
        <main class="main">
            <!-- Start of Breadcrumb -->
            <nav class="breadcrumb-nav">
                <div class="container">
                   
                </div>
            </nav>
            <!-- End of Breadcrumb -->

            <!-- Start of Pgae Contetn -->
            <div class="page-content mb-8" style="margin-top: 20px;">
                <div class="container">
                    <!-- Start of Vendor Map -->
                  
                    <!-- End of Vendor Map -->

                    <!-- Start of Vendor Filter -->
                    
                    <!-- End of Vendor Filter -->

                    <!-- Start of Vendor Store -->
                    <div class="row cols-lg-3 cols-sm-2">



                        <% for(i=0;i<result[1].length;i++){ %>

                        <div class="store-wrap mb-4">
                            <div class="store store-wcmp br-sm">
                                <figure class="store-banner">
                                    <img src="/images/<%=result[1][i].shop_img2%>" style="width:400px;height:318px" alt="Vendor" 
                                        width="400" height="318" style="background-color: #454b63;" />
                                </figure>
                                <div class="store-content">
                                    <figure class="seller-brand">
                                        <img src="/images/<%=result[1][i].image%>" style="width: 80px;height: 80px;" alt="Brand" width="80" height="80" />
                                    </figure>
                                    <div class="seller-date">
                                        <h4 class="store-title">
                                            <a href="/single-vendor-details?vendorid=<%=result[1][i].id%>"><%=result[1][i].business_name%></a>
                                        </h4>
                                        <ul class="seller-info-list list-style-none">
                                            <li class="store-address">
                                                <i class="w-icon-map-marker"></i>
                                                <p><%=result[1][i].address%></p>
                                            </li>
                                            <li class="store-rating">
                                                <div class="ratings-container">
                                                    <div class="ratings-full">
                                                        <span class="ratings" style="width: 100%;"></span>
                                                        <span class="tooltiptext tooltip-top"></span>
                                                    </div>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!-- End of Store -->
                        </div>

                        <% } %>
                     
                    </div>
                    <!-- End of Vendor Store -->
                </div>
            </div>
            <!-- End of Page Content -->
        </main>
        <!-- End of Main -->

        <!-- Start of Footer -->
        <%- include ('./footer.ejs') %>
