<%- include ('./header.ejs') %>

<section class="pt-5 pb-5 homepage-search-block position-relative">
<div class="container">
<div class="row">
<div class="col-md-8">
<div class="homepage-search-title">
<h1 class="mb-3 text-white text-shadow font-weight-bold">Find best deals in your <span class="font-weight-light">City</span></h1>
<h5 class="mb-5 text-white text-shadow font-weight-normal">Top offers at your City.</h5>
</div>
<div class="homepage-search-form">
<form class="form-noborder" action="/search" method="get">
<div class="form-row">

<div class="col-lg-7 col-md-7 col-sm-12 form-group">
<input type="text" name="search" placeholder="Search for stores, coupons, & offersâ€¦" class="form-control form-control-lg">
</div>
<div class="col-lg-2 col-md-2 col-sm-12 form-group">
<button type="submit" class="btn btn-primary btn-block btn-lg btn-gradient"><i class="icofont-search"></i></button>
</div>
</div>
</form>
</div>
<h6 class="mb-0 mt-1 text-white text-shadow font-weight-normal">E.g. Coupans, Offers, Brands, Stores, Restaurant & more...</h6>
</div>
<div class="col-md-4">
<img src="/images/WebMobile.png" class="img-fluid">
</div>
</div>
</div>
</section>
</div>
<section class="section-padding homepage-view-offers">
<div class="container">
<div class="row">
<div class="col-md-5 mx-auto text-center">
<h3 class="mb-3">Exclusive Deals</h3>
<p class="text-dark-50 mb-5">Deals build for you.</p>
</div>
</div>
<div class="row">


    <% for(i=0;i<result[3].length;i++) { %>
<div class="col-xl-3 col-sm-6 mb-4">
<div class="custom-card shadow-sm h-100">
<div class="custom-card-image">
<a href="#">
<img class="img-fluid item-img" src="/images/<%=result[3][i].image%>">
</a>

</div>
<div class="p-3 pt-4">
<div class="custom-card-body">
<h6 class="mb-3"><a class="text-black" href="#"><%=result[3][i].short_description%></a></h6>
<p class="text-gray"><i class="icofont-clock-time"></i> Expiry at <%=result[3][i].expiry%></p>
</div>


<% if(result[3][i].isredeem == null || result[3][i].isredeem == '' || result[3][i].isredeem == 'null' || result[3][i].isredeem ==[]){ %>
<div class="custom-card-footer mb-2">

    <a class="btn btn-orange float-right redeem_this_code" style="margin-bottom: 10px;" id="<%=result[3][i].id%>" code="<%=result[3][i].name%>" expiry="<%=result[3][i].expiry%>" vendorid = '<%=result[3][i].vendorid%>'>
        Redeem Deals</a>
        </div>
    <% } else { %>
        <% } %>


</div>
</div>
</div>
<% } %>

</div>
</div>
</section>
<section class="section-padding homepage-coupon">
    <div class="container">
    <div class="row  align-items-center">
    <div class="col-lg-4 col-md-4">
    <div class="homepage-left-block">
    <img class="round-co-icon" src="/new_design/images/active-power.png" alt="Get Badges Icons">
    <h3 class="text-white">Deals of the day</h3>
    <p class="text-white">Grab lightning deals at lightning speed.</p>
    <a class="btn btn-light-white text-white btn-sm rounded-pill" href="#">View more</a>
    </div>
    </div>
    <div class="col-lg-8 col-md-8">
    <div class="owl-carousel owl-theme owl-carousel-three homepage-coupon-carousel">
  
        <% for(i=0;i<result[4].length;i++) { %>
        <div class="item">
    <div class="bg-white p-4 shadow-sm text-center h-100 border-radius">
    <div class="all-coupon">
    <img class="mb-3 user-cou-img" src="/images/<%=result[4][i].image%>" alt="Generic placeholder image">
    <h6 class="mb-4 mt-3 pb-2 text-secondary"><%=result[4][i].short_description%></h6>
    </div>
    <div class="mb-0">
    <p class="mb-0 text-info"><i class="icofont-clock-time"></i> Expiry at <%=result[4][i].expiry%></p>
    </div>
    <% if(result[4][i].isredeem == null || result[4][i].isredeem == '' || result[4][i].isredeem == 'null' || result[4][i].isredeem ==[]){ %>
        <a class="btn btn-sm btn-white ml-auto redeem_this_code" id="<%=result[4][i].id%>" code="<%=result[4][i].name%>" expiry="<%=result[4][i].expiry%>" vendorid = '<%=result[4][i].vendorid%>' style="cursor:pointer">Redeem Deals</a>
        
        <% } else { %>
        
            <% } %>
    </div>
    </div>
  <% } %>
    </div>
    </div>
    </div>
    </div>
    </section>
<section class="section-padding homepage-great-deals bg-white">
<div class="container">
<div class="row">
<div class="col-lg-12">
<div class="owl-carousel owl-theme owl-carousel-one homepage-great-deals-carousel">
<div class="item">
<div class="great-deals-sstore-banners">
<a href="#"><img alt="placeholder image" src="/resize_banner/Build_mart.jpg" class="img-fluid rounded"></a>
</div>
</div>
<div class="item">
<div class="great-deals-sstore-banners">
<a href="#"><img alt="placeholder image" src="/images/Clothing.jpg" class="img-fluid rounded"></a>
</div>
</div>
</div>
</div>
</div>
</div>
</section>
 <section class="section-padding homepage-store-block">
<div class="container">
<div class="row  align-items-center">
<div class="col-lg-9 col-md-8">
<div class="owl-carousel owl-theme owl-carousel-three homepage-coupon-carousel">


    <% for(i=0;i<result[5].length;i++) { %>
    <div class="item">
<div class="custom-card bg-white shadow-sm h-100 stor-card">
<div class="custom-card-image">
<a href="#">
<img class="img-fluid item-img" src="/images/<%=result[5][i].image%>">
</a>
<div class="shape shape-bottom shape-fluid-x svg-shim text-white">
<svg viewBox="0 0 2880 480" fill="none" xmlns="http://www.w3.org/2000/svg">
<path fill-rule="evenodd" clip-rule="evenodd" d="M2160 0C1440 240 720 240 720 240H0V480H2880V0H2160Z" fill="currentColor"></path>
</svg>
</div>
</div>
<div class="p-3">
<div class="custom-card-body">
<h6 class="mb-0"><a class="text-black" href="#"><%=result[5][i].short_description%></a></h6>
<p class="text-gray mb-2">Expiry at <%=result[5][i].expiry%></p>
</div>
</div>
<div class="p-3 border-top">
<div class="custom-card-badge">
    <% if(result[5][i].isredeem == null || result[5][i].isredeem == '' || result[5][i].isredeem == 'null' || result[5][i].isredeem ==[]){ %>
        
        <a class="btn btn-sm btn-white ml-auto redeem_this_code" id="<%=result[5][i].id%>" code="<%=result[5][i].name%>" expiry="<%=result[5][i].expiry%>" vendorid = '<%=result[5][i].vendorid%>' style="cursor:pointer">Redeem Deals</a>
        
        <% } else { %>
        
        
            <% } %>
</div>
</div>
</div>
</div>
<% } %>

</div>
</div>
<div class="col-lg-3 col-md-4">
<div class="homepage-left-block text-right">
<img class="round-co-icon" src="/new_design/images/active-power.png" alt="Get Badges Icons">
<h3 class="text-white">Mega Deals</h3>
<p class="text-white pl-0 pr-0">Offers that you can't miss , Grab them now!
</p>
<a class="btn btn-light-white text-white btn-sm rounded-pill" href="#">View more</a>
</div>
</div>
</div>
</div>
</section>
<section class="section-padding homepage-great-deals bg-white">
<div class="container">
<div class="row">

    <div class="col-lg-6 col-md-6">
<div class="great-deals-sstore-banners">
<a href="#"><img alt="placeholder image" src="/resize_banner/Education1.jpg" class="img-fluid rounded"></a>
</div>

<div class="great-deals-sstore-banners">
<a href="#"><img alt="placeholder image" src="/resize_banner/Grocery1.jpg" class="img-fluid rounded"></a>
</div>
</div>
<div class="col-lg-6 col-md-6">
    <div class="great-deals-sstore-banners">
    <a href="#"><img alt="placeholder image" src="/resize_banner/Cafe.jpg" class="img-fluid rounded"></a>
    </div>
    
    <div class="great-deals-sstore-banners">
    <a href="#"><img alt="placeholder image" src="/resize_banner/Bakery1.jpg" class="img-fluid rounded"></a>
    </div>
    </div>
</div>
</div>
</section>
<section class="section-padding homepage-rest-block">
<div class="container">
<div class="row">
<div class="col-md-5 mx-auto text-center">
<h3 class="mb-3 text-white">Top Stores</h3>
<p class="text-white-50 mb-5">Explore Best stores at your City on your Finger Tips. Order Now! & get Exclusive deals and offers.</p>
</div>
</div>
<div class="row">


    <% for(i=0;i<result[6].length;i++) { %>
<div class="col-xl-3 col-sm-6 mb-4">
<div class="custom-card shadow-sm bg-white h-100">
<div class="custom-card-image">
<a href="/single-vendor-details?vendorid=<%=result[6][i].id%>">
<img class="img-fluid item-img" src="https:dealsaaj.com/images/<%=result[6][i].image%>">
</a>
<div class="shape shape-bottom shape-fluid-x svg-shim text-white">
<svg viewBox="0 0 2880 480" fill="none" xmlns="http://www.w3.org/2000/svg">
<path fill-rule="evenodd" clip-rule="evenodd" d="M2160 0C1440 240 720 240 720 240H0V480H2880V0H2160Z" fill="currentColor"></path>
</svg>
</div>
</div>
<div class="p-3">
<div class="custom-card-body">
<h6 class="mb-1"><a class="text-black" href="/single-vendor-details?vendorid=<%=result[6][i].id%>"><%=result[6][i].business_name%></a></h6>
</div>
<div class="custom-card-badge mt-2">
    <%=result[6][i].address%>
</div>
</div>
</div>
</div>
<% } %>
</div>
<div class="text-center mt-4">
<a class="btn btn-light-white text-white btn-sm rounded-pill" href="#">View more</a>
</div>
</div>
</section>
<%- include ('./footer.ejs') %>
