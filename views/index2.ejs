<script>
    $('.redeem_this_code').click(function(){
        let coupounid = $(this).attr('id')
        let code = $(this).attr('code')
        let expiry = $(this).attr('expiry')
        let vendorid = $(this).attr('vendorid')
        $.post('/redeem-this-code',{coupounid,code,expiry,vendorid},data=>{
            if(data.msg=='failed') window.location.href = '/login';
            else window.location.reload()
        })
    })
</script>

