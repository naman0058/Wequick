                            <div class="tab-pane" id="edit-details">
                                <div class="icon-box icon-box-side icon-box-light">
                                    <span class="icon-box-icon icon-account mr-2">
                                        <i class="w-icon-user"></i>
                                    </span>
                                    <div class="icon-box-content">
                                        <h4 class="icon-box-title mb-0 ls-normal">Account Details</h4>
                                    </div>
                                </div>
                                <form class="form account-details-form" action="/myaccount-update" method="post" style="margin-top:20px;">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="firstname">Full name *</label>
                                                <input type="text" id="firstname" name="name" placeholder="Enter Name"
                                                    class="form-control form-control-md" value="<%=result[2][0].name%>">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="lastname">Mobile Number *</label>
                                                <input type="text" id="lastname" name="number" placeholder="Mobile Number"
                                                    class="form-control form-control-md" value="<%=result[2][0].number%>" readonly>
                                            </div>
                                        </div>
                                    </div>

                                 

                                    <div class="form-group mb-6">
                                        <label for="email_1">Email address *</label>
                                        <input type="email" id="email_1" name="email" value="<%=result[2][0].email%>"
                                            class="form-control form-control-md">
                                    </div>
                                  
                                   
                                    <button type="submit" class="btn btn-dark btn-rounded btn-sm mb-4">Save Changes</button>
                                </form>
                            </div>



                            <div class="tab-pane" id="account-details">
                                <div class="icon-box icon-box-side icon-box-light">
